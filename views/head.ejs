        }
        nav ul{
            width:60%;
            display: flex;
            justify-content: space-between;
            float:right;
            margin-right: 20px;
        }
        nav ul li{
            display:inline-block;
            line-height: 80px;
            margin: 0 5px;
        }
        nav ul li a:hover{
            transition: 0.2s;
